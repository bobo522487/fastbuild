{
  "extends": "@workspace/typescript-config/trpc.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "types": [],
    "lib": ["es2020", "dom"],
    "exactOptionalPropertyTypes": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictNullChecks": false,
    "strictPropertyInitialization": false
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
