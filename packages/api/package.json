{
  "name": "@workspace/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./trpc": {
      "types": "./dist/trpc/trpc.d.ts",
      "default": "./trpc/trpc.js"
    },
    "./trpc/context": {
      "types": "./dist/trpc/context.d.ts",
      "default": "./trpc/context.js"
    },
    "./trpc/routers": {
      "types": "./dist/trpc/routers/index.d.ts",
      "default": "./trpc/routers/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "default": "./middleware/index.js"
    },
    "./middleware/index": {
      "types": "./dist/middleware/index.d.ts",
      "default": "./middleware/index.js"
    },
    "./middleware/rateLimiter": {
      "types": "./dist/middleware/rateLimiter.d.ts",
      "default": "./middleware/rateLimiter.js"
    },
    "./middleware/errorHandler": {
      "types": "./dist/middleware/errorHandler.d.ts",
      "default": "./middleware/errorHandler.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:contracts": "vitest --config vitest.contracts.config.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/next": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@workspace/database": "workspace:*",
    "@workspace/schema-compiler": "workspace:*",
    "jsonwebtoken": "^9.0.2",
    "next-auth": "^5.0.0-beta.25",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^15.0.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-turbo": "^2.5.5",
    "jsdom": "^24.1.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.39.0",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.40.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
