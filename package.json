{
	"name": "fastbuild",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"build:prod": "SKIP_ENV_VALIDATION=true next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"docs:generate": "next-openapi-gen generate",
		"docs:serve": "next dev & pnpm docs:generate",
		"db:init": "./scripts/migrate.sh init",
		"db:migrate": "./scripts/migrate.sh migrate",
		"db:deploy": "./scripts/migrate.sh deploy",
		"db:reset": "./scripts/migrate.sh reset",
		"db:seed": "./scripts/migrate.sh seed",
		"db:check": "./scripts/db-check.sh --all",
		"db:report": "./scripts/db-check.sh --report",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"dev": "next dev --turbo",
		"postinstall": "prisma generate",
		"preview": "next build && next start",
		"start": "next start",
		"test": "vitest",
		"test:unit": "vitest run src/**/*.test.{js,ts,jsx,tsx}",
		"test:integration": "vitest run tests/integration/**/*.test.{js,ts,jsx,tsx}",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:coverage": "vitest --coverage",
		"db:start": "docker compose up -d",
		"db:start:test": "docker compose up -d postgres-test",
		"db:stop": "docker compose down",
		"db:setup:test": "./scripts/setup-test-db.sh",
		"typecheck": "tsc --noEmit"
	},
