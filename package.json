{
  "name": "create-t3-turbo",
  "private": true,
  "engines": {
    "node": ">=22.20.0",
    "pnpm": ">=10.15.1"
  },
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "auth:generate": "pnpm -F @fastbuild/auth generate",
    "db:push": "turbo -F @fastbuild/db db:push",
    "db:push:test": "pnpm --filter @fastbuild/db db:push:test",
    "db:studio": "turbo -F @fastbuild/db db:studio",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo -F @fastbuild/db db:migrate",
    "db:reset": "turbo -F @fastbuild/db db:reset",
    "db:reset:test": "pnpm --filter @fastbuild/db db:reset:test",
    "dev": "turbo watch dev --continue",
    "dev:next": "turbo watch dev -F @fastbuild/nextjs...",
    "biome:check": "turbo run biome:check --continue",
    "biome:fix": "turbo run biome:fix --continue",
    "lint:ws": "pnpm dlx sherif@latest",
    "postinstall": "pnpm lint:ws",
    "typecheck": "turbo run typecheck",
    "ui-add": "turbo run ui-add",
    "test": "turbo run test",
    "test:integration": "dotenv -e .env.test -- pnpm --filter @fastbuild/api test:integration",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test:coverage",
    "test:ui": "turbo run test:ui"
  },
  "devDependencies": {
    "@turbo/gen": "^2.5.8",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "dotenv-cli": "^10.0.0",
    "jsdom": "^23.0.0",
    "turbo": "^2.5.8",
    "typescript": "catalog:",
    "vitest": "^1.0.0"
  }
}
