{
  "name": "shadcn-ui-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "vitest",
    "test:integration": "vitest --config vitest.integration.config.mts",
    "test:unit": "vitest --config vitest.unit.config.mts",
    "test:performance": "vitest --config vitest.performance.config.mts",
    "test:react": "vitest --config vitest.react.config.mts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^15.0.7",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.6.6",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint": "^9.32.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "shadcn": "^3.3.1",
    "turbo": "^2.5.5",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@auth/pg-adapter": "^1.7.4",
    "@prisma/client": "^6.16.2",
    "@trpc/client": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@trpc/server/adapters/next": "^11.0.0",
    "authjs": "npm:next-auth@^5.0.0-beta.29",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "superjson": "^2.2.1",
    "zod": "^4.1.11"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@prisma/client"
    ]
  }
}
